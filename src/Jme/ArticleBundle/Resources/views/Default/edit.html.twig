{% extends 'JmeArticleBundle::layout.html.twig' %}

{% block content %}

<h1>{{'article.update'|trans}}</h1>

<div id="new-article">
    <form action="{{ path('jme_article_update', {article: form.vars['value'].id})  }}" method="post" {{ form_enctype(form) }} novalidate>
        {{ form_widget(form) }}

        <input type="submit" />
    </form>
</div>
{% endblock %}