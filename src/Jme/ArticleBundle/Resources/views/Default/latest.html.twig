{% extends 'JmeArticleBundle::layout.html.twig' %}

{% block title %}{{ jme_page_title("") }}{% endblock %}

{% block content %}

    {% if articles|length > 0 %}
        <div class="content padded-content" id="a-home">
            {% for article in articles %}
                {% include 'JmeArticleBundle:Default:article.html.twig' with {article: article, listMode: true} %}
            {% endfor %}
        </div>
    {% endif %}
    {% include 'JmeMainBundle:Default:about-inner.html.twig' %}

    {% include 'JmeMainBundle:Default:projects-inner.html.twig' %}

{% endblock %}